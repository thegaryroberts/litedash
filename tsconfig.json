{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "declaration": true,
        "target": "esnext",
        "allowJs": true,
        "noEmit": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strict": true,
        "jsx": "react",
        "module": "esnext",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "baseUrl": ".",
        "paths": {
            "Assets/*": [
                "src/assets/*"
            ],
            "Components/*": [
                "src/components/*"
            ],
            "Demos": [
                "src/demos/index.js"
            ],
            "Functions/*": [
                "src/functions/*"
            ],
            "Interfaces/*": [
                "src/interfaces/*"
            ],
            "Pages": [
                "src/pages/index.js"
            ],
            "Pages/*": [
                "src/pages/*"
            ],
            "States/*": [
                "src/states/*"
            ],
            "Themes": [
                "src/themes/index.js"
            ],
            "Themes/*": [
                "src/themes/*"
            ],
        },
    },
    "include": [
        "index.js",
        "src"
    ],
}
