{
  "presets": [
    [
      "@babel/env",
      {
        "modules": false,
        "targets": "> 0.25%, not dead",
        "useBuiltIns": "usage"
      }
    ],
    "@babel/react",
    "@babel/typescript"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "extensions": [
          ".js",
          ".ts",
          ".tsx"
        ],
        "root": [
          "./src"
        ],
        "alias": {
          "Assets": "./src/assets",
          "Components": "./src/components",
          "Functions": "./src/functions",
          "Interfaces": "./src/interfaces",
          "Pages": "./src/pages",
          "States": "./src/states",
          "Themes": "./src/themes"
        }
      }
    ],
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ]
}
